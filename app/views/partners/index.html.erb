<div class="row heading">
  <div class="col-md-12">
    <h2>Partner Listing</h2>
  </div>
</div>

<div class="row">
  <div class="col-md-4">
    <h3>Partner Name</h3>
  </div>
  <div class="col-md-8">
    <h3>Registration Link</h3>
  </div>
  <% @partners.each do |p| %>
    <div class="col-md-4">
      <h5><%= p.name %></h5>
    </div>
    <div class="col-md-8">
      <h5><a href="/registration/<%= p.id %>"><%= p.id %></a></h5>
    </div>
  <% end %>
</div>
<p>&nbsp;</p>
<div class="row">
  <div class="col-md-12">
    <%= link_to 'Register New Partner', '/partners/new', class: "btn btn-primary-outline btn-sm" %>
  </div>
</div>
